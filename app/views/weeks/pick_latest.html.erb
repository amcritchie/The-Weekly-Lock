<div class="container">
  <!-- Game filter -->
  <div class="input-group"> <span class="input-group-addon">Filter</span>
    <input id="filter" type="text" class="form-control" placeholder="Type here...">
  </div>
  <!-- Matchups -->
  <ul class="flex-container searchable">
    <% @week.games.each do |game| %>
    <li class="flex-item game-box">
      <%= render :template => "games/game_box", :locals => { :game => game, show_result: false, show_spread: true, show_pick: true } %>
    </li>
    <% end %>
    <%= render :template => "games/performance_modal" %>
  </ul>
</div>
