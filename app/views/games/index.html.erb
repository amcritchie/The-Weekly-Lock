<div class="hidden">
  <div id="season" data-year="<%= @season.year %>"></div>
  <div id="week" data-position="<%= @week.position %>"></div>
</div>
<!-- Pager Content -->
<div class="container">

  <div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
      <h4>Actions</h4>
      <a id="build_weekly_matchups" class="btn btn-success">Build Matchups</a>
    </div>
  </div>
  <div class="row">
    <% @games.each do |game| %>
    <div class="col-md-3 col-sm-3 col-xs-6" style="text-align: center;">
      <h4><%= game.away_team.name %> @ <%= game.home_team.name %></h4>
    </div>
    <% end %>
  </div>
  <%= render template: "layouts/footer" %>
</div>
