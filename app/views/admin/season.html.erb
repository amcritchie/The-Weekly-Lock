<div class="container">

  <div class="hidden">
    <div id="season" data-year="2015"></div>
  </div>

  <div class="container">
    <h2>NFL <%= @season.year %> Season</h2>
    <table class="table table-hover table-condensed">
      <thead>
        <tr>
          <th>Week</th>
          <th>Games</th>
        </tr>
      </thead>
      <tbody>
        <% @season.weeks.each do |week| %>
          <tr>
            <td><a href="/admin/nfl/<%= @season.year %>/<%= week.position %>"><%= week.position %></a></td>
            <td><%= week.games.length %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>

  <div class="row">
    <div class="col-md-12 col-sm-12 col-xs-12">
      <h4>Actions</h4>
      <a id="build_seasons_matchups" class="btn btn-success">Build Matchups</a>
      <a id="build_seasons_results" class="btn btn-success">Build Results</a>
      <!-- <a id="build_seasons_results" class="btn btn-primary">Build Results</a> -->
    </div>
  </div>

</div>
