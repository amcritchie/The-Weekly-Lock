<div class="container">
  <!-- Game filter -->
  <div class="input-group"> <span class="input-group-addon">Filter</span>
    <input id="filter" type="text" class="form-control" placeholder="Type here...">
  </div>
  <!-- Matchups -->
  <ul class="flex-container searchable">
    <% @picks.each do |pick| %>
    <% game = pick.performance.game %>
    <li class="flex-item game-box">
      Week <%= game.week.position %>
      <%= render :template => "games/game_box", :locals => { :game => game, show_result: true, show_spread: true, show_pick: true } %>
    </li>
    <% end %>
  </ul>
</div>
